.district
  background-color: #fff
  margin: 20px 0
  .district-header
    padding: 5px
    color: #333
    border: 1px solid #ddd
    background: #F0F0F0
    b.name
      font-size: 14px
      font-weight: bold
      display: inline-block
    a
      color: #2476E9
      font-size: 12px
      margin-left: 10px
    .address
      display: inline-block
      font-size: 12px
      margin-left: 5px
      color: #333
      max-width: 190px
      overflow: hidden
      white-space: nowrap
      text-overflow: ellipsis
      line-height: 12px
    b
      color: #333
      margin: 0 2px
  .district-body
    padding: 10px 30px 10px 10px
    background-color: #fff
    &.floors
      padding: 0
    .floors
      border-bottom: 1px solid #ddd
      border-right: 1px solid #ddd
      position: relative
      padding-bottom: 15px
      .left
        position: absolute
        left: 0
        top: 0
        height: 100%
        width: 40px
        background-color: #eee
        .con
          position: absolute
          width: 100%
          font-size: 12px
          top: 50%
          margin-top: -24px
          line-height: 24px
          text-align: center
          letter-spacing: -1px
          .p2
            color: #999
            span
              color: #000
      .right
        padding-left: 40px

.room-cube-container
  display: inline-block
  float: left

.room-cube
  display: inline-block
  overflow: hidden
  width: 100px
  margin: 17px 0 0 17px
  border: 1px solid #e5e7f0
  border-radius: 4px
  background-color: #fff
  font-size: 12px
  cursor: pointer

  p
    text-overflow: ellipsis
    white-space: nowrap
    overflow: hidden
    padding: 0 5px
  .title
    text-align: center
    span
      font-size: 14px
      border: 1px solid #fff
      border-radius: 3px
      padding: 0 10px
      display: inline
  .state-bg
    position: relative
    height: 92px
    padding: 5px
    background: no-repeat center 80%
    &.state-bg1
      background-color: #2476E9
      color: #fff
    &.state-bg2
      color: #fff
  .bottom
    line-height: 25px
    color: #333

  .stateLabels
    bottom: 5px
    margin: 20px 0 0 3px
    .label
      em
        width: 30px
        display: block
      &.red
        background: #FF5C5C
        span
          color: #FF5C5C
          background-color: #fff
          line-height: 16px
          display: block
          font-size: 12px
      &.blue
        background: #2476E9
        span
          color: #2476E9
          background-color: #fff
          line-height: 16px
          text-align: center
          display: block
          font-size: 12px
      &.green
        background: #08C664
        span
          color: #08C664
          background-color: #fff
          line-height: 16px
          text-align: center
          display: block
          font-size: 12px
      &.other
        background: #8A85FF
        span
          color: #8A85FF
          background-color: #fff
          line-height: 16px
          text-align: center
          display: block
          font-size: 12px
      &.yellow
        background: #FFA42F
        span
          color: #FFA42F
          background-color: #fff
          line-height: 16px
          text-align: center
          display: block
          font-size: 12px
      &.lightBlue
        background: #1DD4DC
        span
          color: #1DD4DC
          background-color: #fff
          line-height: 16px
          text-align: center
          display: block
          font-size: 12px
      display: inline-block
      border-radius: 2px
      text-align: center
      overflow: hidden
      vertical-align: top
      em
        font-size: 14px
        font-weight: bold

.aside
  width: 285px
  background: #F3F3F3
  padding-left: 10px
  position: fixed
  top: 0px
  padding-top: 60px
  bottom: 0px
  right: 0
  .con
    background: #fff
    height: 100%
    .ch2
      color: #2476E9
      font: bold 14px/40px 微软雅黑
      border-bottom: 1px solid #ddd
      padding-left: 10px
    .search
      .tit
        padding: 7px 0
        span
          color: #333
          font: 14px/40px 微软雅黑
          padding: 0 10px
          float: left
        .clean
          border: 1px solid #FE493F
          color: #FE493F
          float: right
          padding: 1px 12px
          border-radius: 3px
          margin: 8px 10px 0 0
      .inp
        border: 1px solid #A9CDFF
        font: 12px/32px 微软雅黑
        margin: 0 10px
        border-radius: 2px
        overflow: hidden

        input
          font: 12px/30px 微软雅黑
          width: 220px
          padding-left: 10px
          float: left
          border: none

