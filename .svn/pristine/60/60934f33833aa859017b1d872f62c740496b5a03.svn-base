      },
      async getYdData(roomId,status) {
        console.log(roomId)
        this.dialogFormVisible = true
        this.ydEdit = status
        const resData = await axios.get('/api/apartmentBooking/getApartmentInfo?roomId='+roomId)
        if(resData.code == '200'){
